{
  "name": "NeoNet Notes",
  "short_name": "Notes",
  "description": "Aplicativo de notas pessoais com sincronização P2P e criptografia local",
  "start_url": "/mock-dapps/neonet-notes/index.html",
  "display": "standalone",
  "background_color": "#4CAF50",
  "theme_color": "#4CAF50",
  "icons": [
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "192x192",
      "type": "image/svg+xml"
    }
  ],
  "permissions": [
    "storage",
    "encryption"
  ],
  "neonet": {
    "version": "1.0.0",
    "offline_capable": true,
    "p2p_enabled": true,
    "storage_quota": "50MB",
    "encryption_required": true
  }
}

