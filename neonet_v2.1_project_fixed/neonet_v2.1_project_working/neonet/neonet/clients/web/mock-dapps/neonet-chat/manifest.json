{
  "name": "NeoNet Chat",
  "short_name": "Chat",
  "description": "Sistema de chat descentralizado que funciona offline",
  "start_url": "/mock-dapps/neonet-chat/index.html",
  "display": "standalone",
  "background_color": "#2196F3",
  "theme_color": "#2196F3",
  "icons": [
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "192x192",
      "type": "image/svg+xml"
    }
  ],
  "permissions": [
    "storage",
    "p2p-communication"
  ],
  "neonet": {
    "version": "1.0.0",
    "offline_capable": true,
    "p2p_enabled": true,
    "storage_quota": "10MB"
  }
}

